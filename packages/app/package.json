{
  "name": "@uminily/app",
  "version": "0.11.2",
  "license": "MIT",
  "repository": "https://github.com/Uminily/kodexo",
  "scripts": {
    "clean": "rm -rf build",
    "build": "rimraf ./dist && tsc -b tsconfig.build.json",
    "test": "jest --runInBand --colors",
    "test:watch": "jest --runInBand --colors --watch",
    "test:start": "ts-node-dev ./tests/mocks/start.ts"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testTimeout": 7000,
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/jest": "^26.0.24",
    "@types/pino-http": "^5.4.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "expect-cookies": "^0.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@tinyhttp/app": "^1.3.11",
    "@tinyhttp/cors": "^1.1.7",
    "@uminily/common": "^0.12.1",
    "@uminily/config": "^0.5.2",
    "@uminily/errors": "^0.1.1",
    "@uminily/injection": "^0.4.1",
    "@uminily/logger": "^0.3.2",
    "@uminily/queueing": "^0.1.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "globby": "^11.0.2",
    "normalize-path": "^3.0.0",
    "normalize-url": "^7.0.0",
    "supertest-fetch": "^1.4.3",
    "type-fest": "^0.20.2"
  },
  "gitHead": "d2c098524d260f4349d9ce38925b4a504c6da172"
}
