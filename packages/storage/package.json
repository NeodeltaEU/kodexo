{
  "name": "@kodexo/storage",
  "version": "1.0.0-dev.50",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "repository": "https://github.com/Uminily/kodexo",
  "scripts": {
    "clean": "rm -rf build",
    "build": "rimraf ./dist && tsc -b tsconfig.build.json",
    "test": "jest --runInBand --colors --forceExit",
    "test:watch": "jest --runInBand --colors --verbose --forceExit --watch",
    "test:start": "ts-node-dev ./tests/mocks/start.ts"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testTimeout": 7000,
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/busboy": "^0.3.1",
    "@types/bytes": "^3.1.1",
    "@types/jest": "^28.1.1",
    "@types/mime-types": "^2.1.1",
    "@types/s3rver": "^3.7.0",
    "@types/stream-meter": "^0.0.22",
    "@types/uniqid": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "form-data": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "s3rver": "^3.7.1",
    "supertest": "^6.1.6",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.44.0",
    "@aws-sdk/lib-storage": "^3.44.0",
    "@kodexo/app": "^1.0.0-dev.50",
    "@kodexo/common": "^1.0.0-dev.50",
    "@kodexo/config": "^1.0.0-dev.50",
    "@kodexo/errors": "^1.0.0-dev.50",
    "@kodexo/injection": "^1.0.0-dev.50",
    "busboy": "^0.3.1",
    "bytes": "^3.1.1",
    "mime-types": "^2.1.34",
    "stream-meter": "^1.0.4",
    "stream-mime-type": "^1.0.2",
    "type-fest": "^2.8.0",
    "uniqid": "^5.4.0"
  },
  "gitHead": "3341d44d2cc6d1a4e6b993d4df43a23a0df3175a"
}
