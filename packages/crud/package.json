{
  "name": "@uminily/crud",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "repository": "https://github.com/Uminily/kodexo",
  "scripts": {
    "build": "rimraf ./dist && tsc -b tsconfig.build.json",
    "test": "jest --coverage --runInBand --colors --forceExit",
    "test:watch": "jest --runInBand --colors --verbose --forceExit --watch",
    "test:coverage": "jest --coverage --runInBand --colors --verbose --forceExit",
    "test:start": "ts-node-dev ./tests/mocks/start.ts"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testTimeout": 7000,
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@uminily/app": "^0.1.0",
    "@uminily/common": "^0.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.1.4"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.5.7",
    "@mikro-orm/core": "^4.5.7",
    "@mikro-orm/postgresql": "^4.5.7",
    "@uminily/errors": "^0.1.0",
    "@uminily/mikro-orm": "^0.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "query-string": "^7.0.1",
    "type-fest": "^1.2.1",
    "uuid": "^8.3.2"
  },
  "gitHead": "d2c098524d260f4349d9ce38925b4a504c6da172"
}
